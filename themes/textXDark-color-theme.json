{
	"name": "textXDark",
	"author": "Daniel Kupƒço",
	"comment": "Visual Studio Code dark theme for textX language.",
    "colors": {
		"editorBackground": "#000c18",
		"editorForeground": "#6688cc",

		"tabsContainerBackground": "#1c1c2a",
		"inactiveTabBackground": "#10192c",
		"activityBarBackground": "#051336"
	},
	"tokenColors": [
		{
			"name": "TextX default",
			"scope": "source.textx",
			"settings": {
				"background": "#6688cc",
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Comments",
			"scope": "comment",
			"settings": {
				"foreground": "#fffa99"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#ff9166"
			}
		},
		{
			"name": "Regular expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#56ff3f"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.character",
				"constant.numeric"
			],
			"settings": {
				"foreground": "#f280d0"
			}
		},
		{
			"name": "Built-in constants",
			"scope": "constant.language",
			"settings": {
				"foreground": "#d811a3"
			}
		},
		{
			"name": "User-defined constants",
			"scope": "constant.other",
			"settings": {
				"foreground": "#6d0050"
			}
		},
		{
			"name": "Keyword operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Choice operators",
			"scope": [
				"keyword.operator.choice",
				"keyword.operator.delimit.squarebracket"
			],
			"settings": {
				"foreground": "#f7e229",
				"fontStyle": ""
			}
		},
		{
			"name": "Squarebrackets",
			"scope": [
				"keyword.operator.reference.squarebracket",
				"keyword.operator.delimit.squarebracket"
			],
			"settings": {
				"foreground": "#f7f451",
				"fontStyle": ""
			}
		},
		{
			"name": "Delimiter",
			"scope": [
				"support.property.group.delimit",
				"keyword.operator.property.group.delimiter"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#f78550"
			}
		},
		{
			"name": "Storage types",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#c96bf9"
			}
		},
		{
			"name": "Entity names",
			"scope": "entity.name.entity",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#8cd4ff"
			}
		},
		{
			"name": "Base types",
			"scope": "support.type.basetype",
			"settings": {
				"foreground": "#b54149",
				"fontStyle": "bold italic"
			}
		},
		{
			"name": "Property names",
			"scope": "entity.name.property",
			"settings": {
				"foreground": "#bcff82"
			}
		}
	]
}